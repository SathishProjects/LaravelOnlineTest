@@ -0,0 +1,73 @@
+<?php namespace App\Http\Controllers\admin;
+
+use App\Http\Requests;
+use App\Http\Controllers\Controller;
+use Illuminate\Http\Request;
+use Redirect;
+class CandidateController extends Controller {
+
+        private $_excelCandidateProperty = [
+            0 => "first_name",
+            1 => "email",
+            2 => "mobile",
+            3 => "date_of_birth",
+            4 => "gender",
+            5 => "SSLC_course",
+            6 => "SSLC_percentage",
+            7 => "SSLC_year_of_completion",
+            8 => "HSC_course",
+            9 => "HSC_percentage",
+            10 => "HSC_year_of_completion",
+            11 => "UG_percentage"
+        ];
+        
+        public function __construct() {
+            $this->middleware('auth');
+        }
+        /**
+         * Display a listing of the resource.
+         *
+         * @return Response
+         */
+        public function index()
+        {
+                $candidateModel = new \App\Candidates;
+                $candidateRecords = $candidateModel->all()->toArray();
+                return view('admin.candidateRegistration')->with('candidate', $candidateRecords);
+        }
+        
+        public function save(Request $request) {
+            $fileName   = $request->file('excel_file')->getClientOriginalName(); 
+            $uploadDir  = public_path() . "\\candidate\\";
+
+            if ($request->file('excel_file')->move($uploadDir, $fileName)) {
+                $excelObject = \Maatwebsite\Excel\Facades\Excel::load($uploadDir . $fileName)->toArray();
+                $insertCount = 0;
+                foreach ($excelObject as $excelColumnIteration => $excelColumnValue) {
+                    $input = [];
+                    foreach ($this->_excelCandidateProperty as $fieldIteration => $field) {
+                        if ($this->_excelCandidateProperty[$fieldIteration] == "first_name") {
+                            $input['first_name'] = $excelColumnValue[$fieldIteration];
+                            $input['last_name'] = "";
+                            if (strpos($excelColumnValue[$fieldIteration], ".") || strpos($excelColumnValue[$fieldIteration], " ")) {
+                                preg_match_all("#(.*)(\s|\.)(.*)#i", $excelColumnValue[$fieldIteration], $matches);
+                                $input['first_name'] = $matches[1][0]; //firstname
+                                $input['last_name'] = $matches[3][0]; //lastname (or) initial
+                            }
+                        }
+                        else {
+                            $input[$this->_excelCandidateProperty[$fieldIteration]] = $excelColumnValue[$fieldIteration];
+                        }
+                    }//row pointer
+                    if (isset($input['first_name']) && isset($input['last_name']) && (isset($input['SSLC_percentage']) || isset($input['HSC_percentage']) || isset($input['UG_percentage']))) {
+                        \Illuminate\Support\Facades\DB::unprepared(sprintf("CALL create_candidates_from_excel ('%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', '%s', %s)", $input['first_name'], $input['last_name'], $input['email'], $input['mobile'], $input['gender'], $input['date_of_birth'], $input['SSLC_course'], $input['SSLC_percentage'], $input['SSLC_year_of_completion'], $input['HSC_course'], $input['HSC_percentage'], $input['HSC_year_of_completion'], $input['UG_percentage'], 'active', 'admin', 'admin', '@insertCount'));
+                        $count = \Illuminate\Support\Facades\DB::select("SELECT @insertCount");
+                        if ($count) {    
+                            $insertCount++;    
+                        }    
+                    }                  
+                }//loop on every row
+                return \Redirect::to('candidate')->with('message', sprintf("%d Candidates found in uploaded file", $insertCount));
+            }
+        }
+}
